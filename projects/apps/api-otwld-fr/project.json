{
  "name": "apps-api-otwld-fr",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "projects/apps/api-otwld-fr/src",
  "projectType": "application",
  "targets": {
    "build": {
      "dependsOn": ["^generate"],
      "executor": "@nrwl/webpack:webpack",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/projects/apps/api-otwld-fr",
        "main": "projects/apps/api-otwld-fr/src/main.ts",
        "tsConfig": "projects/apps/api-otwld-fr/tsconfig.app.json",
        "assets": [
          "projects/apps/api-otwld-fr/src/assets"
        ],
        "generatePackageJson": true
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "projects/apps/api-otwld-fr/src/environments/environment.ts",
              "with": "projects/apps/api-otwld-fr/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nrwl/js:node",
      "options": {
        "buildTarget": "apps-api-otwld-fr:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "apps-api-otwld-fr:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "projects/apps/api-otwld-fr/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "projects/apps/api-otwld-fr/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "helm-deploy": {
      "executor": "@otwld/nx-devops:run-commands",
      "configurations": {
        "production": {
          "commands": [
            {
              "command": "helm upgrade api-otwld-fr . --install --dependency-update -f values.yaml -f values-production.yaml -n ecosystem",
              "forwardAllArgs": false
            }
          ]
        },
        "staging": {
          "commands": [
            {
              "command": "helm upgrade api-otwld-fr . --install --dependency-update -f values.yaml -f values-staging.yaml -n staging",
              "forwardAllArgs": false
            }
          ]
        },
        "apain": {
          "commands": [
            {
              "command": "helm upgrade api-otwld-fr . --install --dependency-update -f values.yaml -f values-local.yaml -n apain",
              "forwardAllArgs": false
            }
          ]
        }
      },
      "options": {
        "parallel": false,
        "cwd": "projects/charts/api-otwld-fr",
        "commands": [
          {
            "command": "helm upgrade api-otwld-fr . --install --dependency-update -f values.yaml -f values-development.yaml -n development",
            "forwardAllArgs": false
          }
        ]
      }
    },
    "docker-publish": {
      "executor": "@nx-tools/nx-docker:build",
      "configurations": {
        "production": {
          "tags": [
            "docker.outworld.fr/ecosystem/api-otwld-fr:${TAG_LATEST_FORMAT}",
            "docker.outworld.fr/ecosystem/api-otwld-fr:${PROJECT_VERSION}"
          ]
        },
        "staging": {
          "tags": [
            "docker.outworld.fr/ecosystem/api-otwld-fr:${TAG_LATEST_FORMAT}",
            "docker.outworld.fr/ecosystem/api-otwld-fr:${PROJECT_VERSION}"
          ]
        }
      },
      "options": {
        "push": true,
        "file": "projects/apps/api-otwld-fr/Dockerfile",
        "tags": [
          "docker.outworld.fr/ecosystem/api-otwld-fr:${TAG_LATEST_FORMAT}"
        ]
      }
    },
    "write-version": {
      "executor": "@otwld/nx-devops:write-file",
      "options": {
        "filePath": "projects/apps/api-otwld-fr/.env",
        "content": "PROJECT_VERSION=${version}"
      }
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "preset": "angular",
        "postTargets": [
          "apps-api-otwld-fr:write-version"
        ]
      }
    }
  },
  "tags": [],
  "implicitDependencies": [
    "shared-models"
  ]
}
